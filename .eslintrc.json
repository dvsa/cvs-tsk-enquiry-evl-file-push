{
  "extends": ["@dvsa/eslint-config-ts"],
  "settings": {
    "import/resolver": {
      "typescript": {
        "alwaysTryTypes": true
      }
    }
  },
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ],
    "@typescript-eslint/no-unsafe-assignment": 0,
    "no-void": 0,
    "no-restricted-syntax": 0,
    "prefer-promise-reject-errors": 0,
    "no-await-in-loop": 0
  }
}
